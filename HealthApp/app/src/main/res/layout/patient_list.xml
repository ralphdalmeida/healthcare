<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.SwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/patient_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/patient_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:scrollbars="vertical"
        android:name="techtest.ehealthinnovation.org.healthapp.PatientListFragment"
        app:layoutManager="LinearLayoutManager"
        tools:context="techtest.ehealthinnovation.org.healthapp.PatientListActivity"
        tools:listitem="@layout/patient_list_content" />
</android.support.v4.widget.SwipeRefreshLayout>
